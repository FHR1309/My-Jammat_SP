<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgr"
    tools:context=".EditTime">

    <EditText
        android:id="@+id/ettZuhr"
        android:layout_width="141dp"
        android:layout_height="49dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="54dp"
        android:background="#D1DEDBDB"
        android:backgroundTint="#E0DDD8D8"
        android:ems="10"
        android:fontFamily="@font/acme"
        android:inputType="time"
        android:text="Time"
        android:textAlignment="center"
        android:textColor="#151414"
        android:textColorHint="#070707"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ettFazr"
        tools:text="Time" />

    <TextView
        android:id="@+id/tvHead"
        android:layout_width="0dp"
        android:layout_height="58dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="655dp"
        android:fontFamily="@font/aladin"
        android:foregroundGravity="center_horizontal"
        android:gravity="center_horizontal|center_vertical"
        android:shadowColor="#3C3939"
        android:shadowRadius="@dimen/material_emphasis_medium"
        android:text="Add a New Masjid"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#00FFFFFF"
        android:textColorLink="#BC2A2A"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="104dp"
        android:layout_height="50dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="16dp"
        android:background="#38CFC1"
        android:backgroundTint="#ABE6EAEA"
        android:fontFamily="@font/aladin"
        android:foregroundGravity="center_horizontal"
        android:gravity="center_horizontal|center_vertical"
        android:shadowColor="#3C3939"
        android:shadowRadius="@dimen/material_emphasis_medium"
        android:text="Zuhr"
        android:textColor="#322D06"
        android:textColorLink="#BC2A2A"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="104dp"
        android:layout_height="50dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="19dp"
        android:background="#38CFC1"
        android:backgroundTint="#ABE6EAEA"
        android:fontFamily="@font/aladin"
        android:foregroundGravity="center_horizontal"
        android:gravity="center_horizontal|center_vertical"
        android:shadowColor="#3C3939"
        android:shadowRadius="@dimen/material_emphasis_medium"
        android:text="Asr"
        android:textColor="#322D06"
        android:textColorLink="#BC2A2A"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="104dp"
        android:layout_height="50dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="14dp"
        android:background="#38CFC1"
        android:backgroundTint="#ABE6EAEA"
        android:fontFamily="@font/aladin"
        android:foregroundGravity="center_horizontal"
        android:gravity="center_horizontal|center_vertical"
        android:shadowColor="#3C3939"
        android:shadowRadius="@dimen/material_emphasis_medium"
        android:text="Maghrib"
        android:textColor="#322D06"
        android:textColorLink="#BC2A2A"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="104dp"
        android:layout_height="50dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="15dp"
        android:background="#38CFC1"
        android:backgroundTint="#ABE6EAEA"
        android:fontFamily="@font/aladin"
        android:foregroundGravity="center_horizontal"
        android:gravity="center_horizontal|center_vertical"
        android:shadowColor="#3C3939"
        android:shadowRadius="@dimen/material_emphasis_medium"
        android:text="Esha"
        android:textColor="#322D06"
        android:textColorLink="#BC2A2A"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <EditText
        android:id="@+id/ettFazr"
        android:layout_width="141dp"
        android:layout_height="49dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="54dp"
        android:background="#D1DEDBDB"
        android:backgroundTint="#E0DDD8D8"
        android:ems="10"
        android:fontFamily="@font/acme"
        android:inputType="time"
        android:text="Time"
        android:textAlignment="center"
        android:textColor="#151414"
        android:textColorHint="#070707"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etName"
        tools:text="Time" />

    <EditText
        android:id="@+id/etName"
        android:layout_width="209dp"
        android:layout_height="60dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="101dp"
        android:background="#D1DEDBDB"
        android:backgroundTint="#E0DDD8D8"
        android:ems="10"
        android:fontFamily="serif"
        android:inputType="text"
        android:textAlignment="center"
        android:textColor="#060505"
        android:textColorHighlight="#151212"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvHead"
        tools:text="Masjid Name" />

    <EditText
        android:id="@+id/ettAsr"
        android:layout_width="141dp"
        android:layout_height="49dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="54dp"
        android:background="#D1DEDBDB"
        android:backgroundTint="#E0DDD8D8"
        android:ems="10"
        android:fontFamily="@font/acme"
        android:inputType="time"
        android:text="Time"
        android:textAlignment="center"
        android:textColor="#151414"
        android:textColorHint="#070707"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ettZuhr"
        tools:text="Time" />

    <EditText
        android:id="@+id/ettMaghrib"
        android:layout_width="141dp"
        android:layout_height="49dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="54dp"
        android:background="#D1DEDBDB"
        android:backgroundTint="#E0DDD8D8"
        android:ems="10"
        android:fontFamily="@font/acme"
        android:inputType="time"
        android:text="Time"
        android:textAlignment="center"
        android:textColor="#151414"
        android:textColorHint="#070707"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ettAsr"
        tools:text="Time" />

    <EditText
        android:id="@+id/ettEsha"
        android:layout_width="141dp"
        android:layout_height="49dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="54dp"
        android:background="#D1DEDBDB"
        android:backgroundTint="#E0DDD8D8"
        android:ems="10"
        android:fontFamily="@font/acme"
        android:inputType="time"
        android:text="Time"
        android:textAlignment="center"
        android:textColor="#151414"
        android:textColorHint="#070707"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ettMaghrib"
        tools:text="Time" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="104dp"
        android:layout_height="50dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="27dp"
        android:background="#38CFC1"
        android:backgroundTint="#ABE6EAEA"
        android:fontFamily="@font/aladin"
        android:foregroundGravity="center_horizontal"
        android:gravity="center_horizontal|center_vertical"
        android:shadowColor="#3C3939"
        android:shadowRadius="@dimen/material_emphasis_medium"
        android:text="Fazr"
        android:textColor="#322D06"
        android:textColorLink="#BC2A2A"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etName" />

    <Switch
        android:id="@+id/swIsActive"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:layout_marginStart="161dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="162dp"
        android:layout_marginBottom="52dp"
        android:background="#E11313"
        android:backgroundTint="#F7F6F4"
        android:gravity="center"
        android:text="Active"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnY"
        app:layout_constraintVertical_bias="0.342" />

    <Button
        android:id="@+id/btnN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:background="@color/teal_200"
        android:backgroundTint="@color/white"
        android:onClick="cancel"
        android:text="Discard"
        android:textColor="#F1EAEA"
        app:iconTint="#EB1A1A"
        app:layout_constraintBottom_toBottomOf="@+id/btnY"
        app:layout_constraintStart_toEndOf="@+id/btnY"
        app:layout_constraintTop_toTopOf="@+id/btnY"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginTop="36dp"
        android:background="@color/teal_200"
        android:backgroundTint="@color/white"
        android:onClick="addMasjid"
        android:text="Create"
        android:textColor="#F1EAEA"
        app:iconTint="#EB1A1A"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

</androidx.constraintlayout.widget.ConstraintLayout>